<!DOCTYPE html>
<html>
   <head>
      <title>Sign up</title>
   </head>
   <body>
      <h1>Get Started</h1>



      <%= form_for(@user) do |f| %>
          <%= render 'shared/error_messages', :object => f.object %>
          <%= f.label :name %><br />
          <%= f.label :first %><br />
          <%= f.text_field :name %><br />
                     
          <%= f.label :middle %><br />
          <%= f.text_field :middle %><br />

          <%= f.label :last %><br />
          <%= f.text_field :last %><br />

          <%= f.label :email %><br />
          <%= f.text_field :email %><br />
          
          <%= f.label :phone %><br />
          <%= f.text_field :phone %><br />

          <%= f.label :textbox %><br />
          <%= f.text_area :textbox %><br />

          <%= f.label :status %><br />
          <%= f.text_field :status %><br />

          <%= f.submit "Submit" %>
      <% end %>
   </body>
</html>

